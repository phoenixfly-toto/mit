{
  "version": "0.3.0",
  "configurations": [
    {
      "type": "gdb",
      "request": "attach",
      "name": "Attach to gdbserver",
      "executable": "${workspaceRoot}/kernel/kernel",
      "gdbpath": "gdb-multiarch",
      "remote": true,
      "target": "127.0.0.1:26001",
      "cwd": "${workspaceRoot}",
      "setupCommands": {
        "text": "source ${workspaceFolder}/.gdbinit"
      }
    },
    {
      "name": "C/C++ Runner: Debug Session",
      "type": "cppdbg",
      "request": "launch",
      "args": [],
      "stopAtEntry": false,
      "externalConsole": false,
      "cwd": "/home/rootuser/MIT/xv6-labs-2020",
      "program": "/home/rootuser/MIT/xv6-labs-2020/build/Debug/outDebug",
      "MIMode": "gdb",
      "miDebuggerPath": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    }
  ]
}